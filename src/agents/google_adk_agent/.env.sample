# Google ADK Agent Environment Variables Sample
# Copy this file to .env in the project root or src/config directory

# ===== Google ADK Configuration =====
# Google API key for accessing Gemini models
GOOGLE_API_KEY=your_google_api_key
# Google ADK model ID
GOOGLE_ADK_MODEL_ID=gemini-2.5-pro-preview-03-25
# Use Vertex AI (TRUE/FALSE)
GOOGLE_GENAI_USE_VERTEXAI=FALSE
